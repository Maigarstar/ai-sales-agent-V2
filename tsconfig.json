{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "app/*"
      ],
      "@/ecme/*": [
        "app/ecme/*"
      ],
      "@/components/*": [
        "app/ecme/components/*",
        "app/components/*"
      ],
      "@/utils/*": [
        "app/ecme/utils/*",
        "app/utils/*"
      ],
      "@/configs/*": [
        "app/ecme/configs/*",
        "app/configs/*"
      ],
      "@/constants/*": [
        "app/ecme/constants/*",
        "app/constants/*"
      ],
      "@/@types/*": [
        "@types/*"
      ],
      "@/lib/*": [
        "app/lib/*"
      ],
      "@/admin/*": [
        "app/admin/*"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".next/dev/types/**/*.ts",
    "**/*.mts",
    "app/vendors-chat-bin/page.tsx-bin",
    "tailwind.config.js",
    "app/lib/supabaseClient.js",
    "app/api/admin/vendor-applications/[id]/route.ts--old"
  ],
  "exclude": [
    "node_modules"
  ]
}
